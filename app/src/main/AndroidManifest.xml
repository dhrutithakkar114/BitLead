<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="itaims.com.bitlead">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"/>
        <activity android:name=".SingleListItemView"
            android:label="BitLead"
            android:theme="@android:style/Theme.Holo.Light.DarkActionBar">
    </activity>
        <activity android:name=".ForgetPass"/>
        <activity android:name=".ForgetOtp"/>
        <activity android:name=".UpdatePass"/>

        <activity android:name=".AddClientInfo"
            android:label="@string/action_add"
            android:parentActivityName=".MainActivity">
        </activity>

    </application>

</manifest>